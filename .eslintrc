{
	"extends": [
		"react-app",
		"airbnb",
		"plugin:jsx-a11y/recommended",
		"prettier",
		"prettier/react"
	],
	"plugins": [
		"jsx-a11y",
		"prettier"
	],
	"ignorePatterns": [
		"serviceWorker.js",
		"setupTests.js",
		"node_modules/"
	],
	"rules": {
		"import/no-named-as-default": 0,
		"semi": 0,
		"react/jsx-props-no-spreading": 0,
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"prettier/prettier": [
			"error",
			{
				"semi": true
			}
		]
	}
}